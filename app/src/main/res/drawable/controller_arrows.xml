<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--UP-->
    <item
        android:top="0dp"
        android:bottom="195dp"
        android:right="100dp"
        android:left="100dp">
